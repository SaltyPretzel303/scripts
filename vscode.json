// save at ~/.config/Code/User/settings.json
{
	"workbench.startupEditor": "newUntitledFile",
	"java.jdt.ls.java.home": "/usr/lib/jvm/default-java",
	"editor.foldingHighlight": false,
	"extensions.ignoreRecommendations": true,
	"workbench.tree.indent": 12,
	"editor.tabCompletion": "on",
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.renderLineHighlight": "line",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.quickSuggestions": {
		"other": false,
		"comments": false,
		"strings": false
	},
	"editor.acceptSuggestionOnEnter": "on",
	"editor.suggestOnTriggerCharacters": false,
	// "editor.quickSuggestionsDelay": 500,
	"editor.wordBasedSuggestions": "off",
	"[java]": {
		// "editor."
	},
	"gitlens.currentLine.enabled": false,
	"gitlens.codeLens.enabled": false,
	"editor.codeLens": false,
	"editor.tokenColorCustomizations": {
		"[i Solarised]": {
			"comments": "#deba6c",
			"strings": "#7e4f0d"
		},
		"[Visual Studio Dark]": {
			// "comments": "#4a6d4a",
			"comments": "#355f35",
			"textMateRules": [
				{
					"scope": [
						"constant.language",
						"entity.name.tag",
						"markup.bold",
						"markup.heading",
						"markup.changed",
						"meta.preprocessor",
						"entity.anme.function.preprocessor",
						"meta.diff.header",
						"storage",
						"storage.type",
						"storage.modifier",
						"keyword.operator.noexcept",
						"punctuation.definition.template-expression.begin",
						"punctuation.definition.template-expression.end",
						"punctuation.section.embedded",
						"keyword",
						"keyword.control",
						// I think next ones are the only ones i want
						"keyword.operator.new",
						"keyword.operator.expression",
						"keyword.operator.cast",
						"keyword.operator.sizeof",
						"keyword.operator.alignof",
						"keyword.operator.typeid",
						"keyword.operator.alignas",
						"keyword.operator.instanceof",
						"keyword.operator.logical.python",
						"keyword.operator.wordlike",
						// down to this line
						"punctuation.section.embedded.begin.php",
						"punctuation.section.embedded.end.php"
					],
					"settings": {
						"foreground": "#ca9e81",
						// "foreground": "#569cd6" original color
						// "foreground": "#569cd6"
					}
				},
				{
					"name": "this.self",
					"scope": "variable.language",
					"settings": {
						"foreground": "#ca9e81"
						// "foreground": "#569cd6" original color
					}
				},
			]
		},
		"[Visual Studio Light]": {
			"comments": "#133a13a4",
		},
		"[Tranquility Base Dark]": {
			"comments": "#f0ceb249"
		},
		"[Tranquility Base Light]": {
			"comments": "#f8ab37b6"
		},
		"[Night]": {
			"comments": "#4f7734"
		}
	},
